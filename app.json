{
  "expo": {
    "name": "expo-random-activity",
    "slug": "expo-random-activity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "schema": "expo-random-activity",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f5f2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "package": "com.up2dul.exporandomactivity",
      "versionCode": 1,
      "supportsTablet": true
    },
    "android": {
      "package": "com.up2dul.exporandomactivity",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f8f5f2"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "653e8196-170a-47e2-a854-96af93c5fafb"
      }
    },
    "owner": "up2dul"
  }
}
